{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "displayName": "Product",
    "singularName": "product",
    "pluralName": "products",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text"
    },
    "price": {
      "type": "decimal"
    },
    "stock": {
      "type": "integer"
    },
    "thumbnail": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "inversedBy": "products"
    },
    "cart_items": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::cart-item.cart-item",
      "mappedBy": "product"
    },
    "order_items": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order-item.order-item",
      "mappedBy": "product"
    }
  }
}
